      "source": [
        "def SelectionSort(x):\n",
        "  \n",
        "  for i in range(len(x)-1):\n",
        "    im = i\n",
        "    for j in range(i,len(x)):\n",
        "      if (x[j]<x[im]):\n",
        "        im = j\n",
        "\n",
        "    x[im],x[i] = x[i],x[im]\n",
        "\n",
        "  return x"
      ]
    },
