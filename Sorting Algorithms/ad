      "source": [
        "def bubsort(x):\n",
        "  count = 0\n",
        "  for i in range(len(x)-1):\n",
        "    for j in range(len(x)-1-i):\n",
        "      if(x[j]>x[j+1]):\n",
        "        x[j],x[j+1] = x[j+1],x[j]\n",
        "        count +=1\n",
        "\n",
        "    if(count==0):\n",
        "      return(\"The list is already sorted\")\n",
        "  return(x)"
      ]
    },
